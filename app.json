{
  "expo": {
    "name": "To Do (React Native)",
    "slug": "todo-app-react-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.felipe.salomao.todoappreactnative"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.felipe.salomao.todoappreactnative"
    },
    "extra": {
      "eas": {
        "projectId": "45a35950-f33a-43cc-9b31-ec637ea85291"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}
